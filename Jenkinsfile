pipeline {
    agent any

    stages {
        stage('vijay') {
            steps {
                echo 'welcome to Jenkins Declarative pipelines..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}